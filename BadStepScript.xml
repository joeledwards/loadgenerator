<MyScript>
	<Properties>
		<cacheSize>50</cacheSize>
		<domain>en.wikipedia.org</domain>
		<jobID>0004</jobID>
		<timeout>5000</timeout>
		<waitstep>1000</waitstep>
		<proxyPort>10000</proxyPort>
		<proxyHost>localhost</proxyHost>
		<javaScriptEnabled>false</javaScriptEnabled>
		<redirectEnabled>false</redirectEnabled>
		<throwExceptionOnScriptError>true</throwExceptionOnScriptError>
		<useInsecureSSL>false</useInsecureSSL>
		<popupBlockerEnabled>true</popupBlockerEnabled>
		<htmlRobotBrowserVersion>none</htmlRobotBrowserVersion>
	</Properties>
	<STEPS>
		<WAIT length="3000" />
		<INVOKE path="/NoPageHere.htm" />
		<VERIFY_TITLE title="bad_title" />
		<BUTTON name="test_button" />
	</STEPS>
</MyScript>