<MyScript>





	<Properties>
		<cacheSize>50</cacheSize>
		<domain>www.customercentrix.com</domain>
		<jobID>0001</jobID>
		<timeout>5000</timeout>
		<waitstep>1000</waitstep>
		<proxyHost>localhost</proxyHost>
		<javaScriptEnabled>false</javaScriptEnabled>
		<redirectEnabled>true</redirectEnabled>
		<throwExceptionOnScriptError>true</throwExceptionOnScriptError>
		<useInsecureSSL>false</useInsecureSSL>
		<popupBlockerEnabled>true</popupBlockerEnabled>
		<htmlRobotBrowserVersion>none</htmlRobotBrowserVersion>
		<remoteport>80</remoteport></Properties>

	<STEPS>
		<WAIT length="3000" />
		<INVOKE path="" />
		<VERIFY_TITLE
			title="CustomerCentrix | We satisfy your business technology needs" />

		<WAIT length="3000" />
		<INVOKE path="/about_us.htm" />
		<VERIFY_TITLE title="About CustomerCentrix | CustomerCentrix" />
		<WAIT length="3000" />
		<INVOKE path="/contact.htm" />
		<VERIFY_TITLE title="Contact Information | CustomerCentrix" />
		<WAIT length="2000" />
		<INVOKE path="/products.htm" />
		<VERIFY_TITLE title="Products | CustomerCentrix" />
		<WAIT length="1000" />
		<INVOKE path="/training.htm" />
		<VERIFY_TITLE title="Training | CustomerCentrix" />
		<WAIT length="4000" />
		<INVOKE path="/consulting.htm" />
		<VERIFY_TITLE title="Consulting | CustomerCentrix" />
		<WAIT length="2000" />
		<INVOKE path="/testimonials.htm" />
		<VERIFY_TITLE title="Testimonials | CustomerCentrix" />
		<WAIT length="1000" />

		<POST path="" name="LoadGenerator" pass="LoadGenerator"
			op="Log in" />
		<VERIFY_TITLE
			title="CustomerCentrix | We satisfy your business technology needs" />



	</STEPS>

</MyScript>